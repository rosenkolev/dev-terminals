{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en-GB",
  "overrides": [
    {
      "filename": "{*.cmd,*.cs,*.csproj}",
      "language": "en-US"
    },
	{
      "filename": "*.csproj",
      "words": [
	    "netstandard",
		"Rosen",
		"Kolev",
		"Menees",
		"snupkg",
		"msbuild",
		"stylecop",
		"inprocess"
	  ]
    },
  ],
  "ignorePaths": [
    ".gitattributes",
    ".gitignore",
    ".editorconfig",
	".cspell.json",
	"Menees.Analyzers.Settings.xml",
    "*.{png,jpg,pdf,svg}",
    "**/bin/**",
    "**/obj/**",
    "**/TestResults/**",
    "**/launchSettings.json",
	"**/Properties/IsExternalInit.cs"
  ],
  "words": [
    "cref",
	"Analyzers"
  ]
}